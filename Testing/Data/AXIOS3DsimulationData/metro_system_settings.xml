        <file>bgapi_trace_log.log</file>
      </trace_log>
      <camera>
        <serial_number>0068123810</serial_number>
        <exposure_time>300</exposure_time>
        <max_exposure_time>2500</max_exposure_time>
        <master>1</master>
        <image_format_id>0</image_format_id>
        <flash>1</flash>
        <packet_delay_ticks>50</packet_delay_ticks>
        <trigger_delay>0</trigger_delay>
      </camera>
      <camera>
        <serial_number>0068153810</serial_number>
        <exposure_time>500</exposure_time>
