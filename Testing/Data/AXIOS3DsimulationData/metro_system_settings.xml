<metrology_system_settings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi="noNamespace" format_version="9" >
  <emmett format_version="1" >
    <device initstate="1" type="baumer_acq" name="CamBar B2" >
