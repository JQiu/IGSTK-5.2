    <device initstate="1" type="a3m" name="BlitzController" >
      <configuration>
        <duration>600</duration>
        <intensity>2000</intensity>
        <delay>0</delay>
        <edge_rising>1</edge_rising>
        <temperature_interval>10000</temperature_interval>
        <flashing_time_from_start_led_0>60000</flashing_time_from_start_led_0>
        <flashing_time_from_start_led_1>3000</flashing_time_from_start_led_1>
      </configuration>
    </device>
  </emmett>
  <bunseki>
    <measurementvolume>
      <use type="pyramid" >
        <pyramid>
          <r00>1.0</r00>
          <r01>0.0</r01>
          <r02>0.0</r02>
          <r10>0.0</r10>
          <r11>1.0</r11>
          <r12>0.0</r12>
          <r20>0.0</r20>
          <r21>0.0</r21>
          <r22>1.0</r22>
          <translation_x>0.0</translation_x>
          <translation_y>0.0</translation_y>
          <translation_z>-500.0</translation_z>
          <a>900.0</a>
          <b>800.0</b>
          <c>150.0</c>
          <d>150.0</d>
          <h>1000.0</h>
        </pyramid>
      </use>
      <clippingplanemax>2000.0</clippingplanemax>
    </measurementvolume>
    <acc_classes>
      <point ok_border="4" >
        <_3drms>
          <class id="1" >0.2</class>
          <class id="2" >0.3</class>
          <class id="3" >0.375</class>
          <class id="4" >0.45</class>
