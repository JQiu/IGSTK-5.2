        </circleapproximation>
        <ellipsemeasurement>
          <ellipsecross>
            <blackobjects>0</blackobjects>
            <nearingdistance>10.0</nearingdistance>
            <filtersize>1</filtersize>
            <iterthreshold>1.0E-2</iterthreshold>
            <miniterations>3</miniterations>
            <maxiterations>6</maxiterations>
